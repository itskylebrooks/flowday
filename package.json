{
  "name": "flowday",
  "private": true,
  "version": "1.0.5",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --check \"src/app/**/*.{ts,tsx}\" \"src/features/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\" src/components/AuraBlock.tsx src/components/EmojiPickerModal.tsx src/components/MonthFlow.tsx src/components/WeekTimeline.tsx src/components/ReleaseOverlay.tsx src/components/MonthlyMix.tsx src/components/SettingsModal.tsx eslint.config.mjs vite.config.ts vitest.config.ts",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "dependencies": {
    "html-to-image": "^1.11.13",
    "motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "unicode-emoji-json": "^0.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.1",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
